#!/bin/bash

set -e

echo
echo "===> Build started at $(date)"
echo
START=$SECONDS

# Unconditionally print elapsed build time at exit
function finish {
  echo "===> Elapsed time: $(($SECONDS - $START)) seconds"
}
trap finish EXIT

echo "---> Moving source..."
if [ "$(ls -A /tmp/src)" ]; then
  mv /tmp/src/* ${APP_ROOT}
fi

echo
echo "===> Build completed at $(date)"





